package com.ar.ui.utils.graphics
{

	/**
	 * Enumeration of drawing types.
	 *
	 * @author Alan Ross
	 * @version 0.1
	 */
	public class DrawType
	{
		/**
		 * Flag to set drawing type to fill
		 */
		public static const FILL:int = 0;

		/**
		 * Flag to set drawing type to outline
		 */
		public static const OUTLINE:int = 1;

		/**
		 * Flag to set drawing type to fill and outline
		 */
		public static const FILL_AND_OUTLINE:int = 2;
	}
}
